{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration:backend": "vitest run --testNamePattern='auth-real-backend'",
    "test:integration:firebase": "FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080 vitest run --testNamePattern='auth-firebase-emulator'",
    "test:integration:context": "vitest run --testNamePattern='auth-backend-integration'",
    "firebase:emulators": "firebase emulators:start --only auth,firestore",
    "test:emulated": "FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080 npm run test:integration:firebase",
    "dev:e2e": "dotenv -e .env.e2e -- next dev -p 3001",
    "dev:test": "dotenv -e .env.test -- next dev",
    "build:e2e:prod": "./scripts/build-production-e2e.sh",
    "start:e2e:prod": "dotenv -e .env.production.e2e -- next start",
    "test:e2e": "playwright test -c playwright.config.e2e.ts",
    "test:e2e:ui": "playwright test -c playwright.config.e2e.ts --ui",
    "test:e2e:simple": "playwright test --config=playwright.config.test.ts",
    "test:e2e:simple:ui": "playwright test --config=playwright.config.test.ts --ui",
    "test:e2e:headed": "playwright test -c playwright.config.e2e.ts --headed",
    "test:e2e:debug": "playwright test -c playwright.config.e2e.ts --debug",
    "test:e2e:prod": "playwright test -c playwright.config.production.ts",
    "test:e2e:prod:ui": "playwright test -c playwright.config.production.ts --ui",
    "test:e2e:prod:headed": "playwright test -c playwright.config.production.ts --headed"
  },
